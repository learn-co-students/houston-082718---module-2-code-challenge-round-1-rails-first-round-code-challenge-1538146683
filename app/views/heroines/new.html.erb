<% if flash[:errors] %>
        <% flash[:errors].each do |error| %>
            <h1><%= error %></h1>
            <% end %>
    <% end%>

<h2>New Heroines</h2>

<%= form_with model: @heroine do |f| %>
    <p>Name: <%= f.text_field :name %></p>
    <p>Super Name: <%= f.text_field :super_name %></p>
    <p>Power: <%= f.collection_select :power_id, @powers, :id, :name %></p>
    <%= f.submit %>
<% end %>