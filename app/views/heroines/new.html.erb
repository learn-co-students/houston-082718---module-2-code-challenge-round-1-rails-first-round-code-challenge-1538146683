<h1>New Heroine Form</h1>

<%= form_with model: @heroine do |f| %>
    <label>Super Name: </label><br />
    <%= f.text_field :super_name %><br />
    <label>Name: </label><br />
    <%= f.text_field :name %><br />
    <label>Super Power: </label><br />
    <%= f.collection_select :power_id, Power.all, :id, :name %><br />
    <%= f.submit %>
<% end %>